mixin categories-ul(...items)
    ul
        each item in items
            li: a.categories-nav__item= item

section.catalog
    .catalog-container.container
        .categories-nav
            +categories-ul('Architecture', 'Art & Fashion', 'Biography', 'Business', 'Crafts & Hobbies', 'Drama', 'Fiction', 'Food & Drink', 'Health & Wellbeing', 'History & Politics', 'Humor', 'Poetry', 'Psychology', 'Science', 'Technology', 'Travel & Maps')
        .bookshelf
            .books
            button.btn.btn-load-more Load more
